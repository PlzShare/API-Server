<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.douzone.weboard.vo.Document" alias="document"/>
		<typeAlias type="com.douzone.weboard.vo.Workspaces" alias="workspaces"/>
		<typeAlias type="com.douzone.weboard.vo.WorkspaceUsers" alias="workspaceUsers"/>
    	<typeAlias type="com.douzone.weboard.vo.Channel" alias="channel"/>
		<typeAlias type="com.douzone.weboard.vo.Noti" alias="noti"/>
		<typeAlias type="com.douzone.weboard.vo.NotiUser" alias="notiuser"/>
		<typeAlias type="com.douzone.weboard.vo.Content" alias="content"/>
		<typeAlias type="com.douzone.weboard.vo.User" alias="User" />
		<typeAlias type="com.douzone.weboard.vo.SearchInfo" alias="searchInfo" />
		<typeAlias type="com.douzone.weboard.vo.Document" alias="document" />
		<typeAlias type="com.douzone.weboard.vo.ChangeUser" alias="changeUser" />
		<typeAlias type="com.douzone.weboard.vo.Chatroom" alias="chatroom" />
		<typeAlias type="com.douzone.weboard.vo.ChatroomUsers" alias="chatroomUsers" />
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/mappers/channels.xml" />
		<mapper resource="mybatis/mappers/noti.xml" />
		<mapper resource="mybatis/mappers/notiuser.xml" />
		<mapper resource="mybatis/mappers/user.xml" />
		<mapper resource="mybatis/mappers/document.xml" />
		<mapper resource="mybatis/mappers/workspaces.xml" />
		<mapper resource="mybatis/mappers/workspaceUsers.xml" />
		<mapper resource="mybatis/mappers/chatroom.xml" />
		<mapper resource="mybatis/mappers/chatroomUsers.xml" />
	</mappers>

</configuration>