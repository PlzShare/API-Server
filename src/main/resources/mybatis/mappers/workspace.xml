<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="workspace">	

	<select id="findAll" parameterType="long" resultType="workspace">
		<![CDATA[
		select no, name, created_at as createAt, user_no as userNo
		from workspace where user_no = #{userNo}
		]]>
	</select>
</mapper>