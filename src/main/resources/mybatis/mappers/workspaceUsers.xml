<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="workspace_users">
	<insert id="invite" parameterType="map">
		<if test='user == 0'>
			<![CDATA[
			insert into workspace_user(user_no, workspace_no, role) values(#{userNo}, #{workspaceNo}, 'admin');
			]]>
		</if>
		<if test='user != 0'>
			<![CDATA[
			insert into workspace_user(user_no, workspace_no) values(#{userNo}, #{workspaceNo});
			]]>
		</if>
	</insert>
</mapper>